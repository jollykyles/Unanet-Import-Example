<project name="IMU-Pre-Process" default="pre-process" basedir=".">

   <target name="pre-process"
           description="Custom processing before running imports">


      <echo>Generating CSV file for import...</echo>
      <exec executable="python" failonerror="true">
         <arg value="../move_file.py"/>
      </exec>
      <echo>CSV file generation complete.</echo>

      <echo>Moving the output files to ${run.input.dir}...</echo>
      <move failonerror="false" file="unanet_imu/data/import.csv" todir="${run.input.dir}" />
      <echo>Move complete</echo>

   </target>
</project>
